@model IEnumerable<Model.UserInfo>

@{
    ViewBag.Title = "用户列表";
    ViewBag.Menu = "UserList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UserList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
