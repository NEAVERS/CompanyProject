@model IEnumerable<Model.PassRecord>

@{
    ViewBag.Menu = "PassRecord";
    ViewBag.Title = "CarInOut";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CarInOut</h2>

<p>
    @Html.ActionLink("录入一条记录", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Plate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PassTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WeightRecordId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InUserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CardId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Plate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PassTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WeightRecordId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InUserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CardId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
